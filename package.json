{
   "name": "MyProject",
   "version": "0.0.1",
   "description": "Awesome project developed with TypeORM.",
   "type": "commonjs",
   "devDependencies": {
      "@types/bcrypt": "^5.0.2",
      "@types/cors": "^2.8.17",
      "@types/express": "^4.17.21",
      "@types/node": "^16.18.68",
      "ts-node": "^10.7.0",
      "typescript": "4.5.2"
   },
   "dependencies": {
      "@types/jsonwebtoken": "^9.0.5",
      "@types/yup": "^0.32.0",
      "bcrypt": "^5.1.1",
      "body-parser": "^1.20.2",
      "cors": "^2.8.5",
      "express": "^4.18.2",
      "jsonwebtoken": "^9.0.2",
      "nodemon": "^3.0.2",
      "pg": "^8.4.0",
      "reflect-metadata": "^0.1.13",
      "typeorm": "0.3.17",
      "typeorm-cli": "^1.0.7",
      "yup": "^1.3.3"
   },
   "scripts": {
      "start": "ts-node src/index.ts",
      "typeorm": "typeorm-ts-node-commonjs",
      "dev": "nodemon --exec ts-node src/index.ts",
      "typeorm-cli": "ts-node node_modules/typeorm/cli.js --dataSource src/data-source.ts",
      "db:migrate:sync": "npm run typeorm-cli -- migration:generate -p src/migration/sync",
      "db:migrate:up": "npm run typeorm-cli -- migration:run",
      "db:migrate:down": "npm run typeorm-cli -- migration:revert"
   }
}
